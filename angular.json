{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"hospital-app": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "less"
				}
			},
			"root": "projects/hospital-app",
			"sourceRoot": "projects/hospital-app/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/hospital-app",
						"index": "projects/hospital-app/src/index.html",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "projects/hospital-app/tsconfig.app.json",
						"inlineStyleLanguage": "less",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/hospital-app/public",
								"output": "."
							},
							{
								"glob": "**/*",
								"input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
								"output": "/assets/"
							}
						],
						"styles": [
							"projects/hospital-app/src/styles.less",
							{
								"input": "projects/hospital-app/src/styles/default.less",
								"bundleName": "default",
								"inject": false
							},
							{
								"input": "projects/hospital-app/src/styles/dark.less",
								"bundleName": "dark",
								"inject": false
							}
						],
						"stylePreprocessorOptions": {
							"includePaths": [
								"projects/hospital-app/src/styles/themes"
							]
						},
						"scripts": [
							"node_modules/apexcharts/dist/apexcharts.min.js"
						],
						"main": "projects/hospital-app/src/main.ts",
						"extraWebpackConfig": "projects/hospital-app/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kB",
									"maximumError": "4kB"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/hospital-app/webpack.prod.config.js"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "hospital-app:build:production",
							"extraWebpackConfig": "projects/hospital-app/webpack.prod.config.js"
						},
						"development": {
							"buildTarget": "hospital-app:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4200,
						"publicHost": "http://localhost:4200",
						"extraWebpackConfig": "projects/hospital-app/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/hospital-app/tsconfig.spec.json",
						"inlineStyleLanguage": "less",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/hospital-app/public"
							}
						],
						"styles": [
							"./node_modules/ng-zorro-antd/ng-zorro-antd.min.css",
							"projects/hospital-app/src/styles.less"
						],
						"scripts": []
					}
				}
			}
		},
